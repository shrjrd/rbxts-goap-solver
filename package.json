{
  "name": "@rbxts/goap-solver",
  "version": "0.4.0",
  "description": "A lightweight, pure functional GOAP solver in TypeScript for creating complex AI agent behavior in games and simulations",
  "homepage": "https://github.com/shrjrd/goap-solver#readme",
  "keywords": [
    "goap",
    "ai",
    "game",
    "simulation"
  ],
  "author": "Alexey Ozerov",
  "license": "MIT",
  "type": "module",
  "types": "out/index.d.ts",
  "main": "out/init.lua",
  "files": [
    "out",
    "!**/*.tsbuildinfo",
    "!out/*.spec.*",
    "!out/jest.*"
  ],
  "scripts": {
    "clean": "del-cli ./out",
    "build": "npm run clean && rbxtsc",
    "build:dev": "npm run clean && rbxtsc --type game --rojo default.project.json",
    "build:rbxl": "npm run build:dev && rojo sourcemap -o sourcemap.json &&rojo build -o place.rbxl",
    "test": "jestrbx",
    "semantic-release": "semantic-release",
    "publish": "npm publish --access=public",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^3.0.0-types.0",
    "@rbxts/coverage": "^0.1.2",
    "@rbxts/jest": "^3.13.3-alpha.2",
    "@rbxts/jest-globals": "^3.13.3-alpha.2",
    "@rbxts/types": "^1.0.894",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-roblox-ts": "^1.3.0",
    "jest-roblox-assassin": "github:Unreal-Works/jest-roblox-assassin",
    "prettier": "^3.7.0",
    "roblox-ts": "^3.0.0",
    "rojo": "^1.0.3-rojo7.6.1",
    "semantic-release": "^24.2.1",
    "typescript": "^5.9.3"
  },
  "release": {
    "branches": [
      "main"
    ],
    "dryRun": true,
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shrjrd/goap-solver.git"
  },
  "bugs": {
    "url": "https://github.com/shrjrd/goap-solver/issues"
  }
}
